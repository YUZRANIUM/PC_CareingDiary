{
	"editor.fontFamily": "MyricaM M",
	"editor.fontSize": 16,
	"editor.renderWhitespace": "all",
	"editor.multiCursorModifier": "ctrlCmd",
	"editor.insertSpaces": false,
	"files.exclude": {
		"{!/*.db,!/*.sql}": true,
		"hsp36": true,
		"hsp37beta": true,
		"hsptmp": true,
		"obj": true,
		"Project": true,
		"SQL": true,
		"vsave*": true
	},
	"editor.autoClosingDelete": "always",
	"editor.autoClosingOvertype": "always",
	"editor.autoClosingBrackets": "languageDefined",
	"editor.codeLensFontFamily": "MyricaM M",
	"editor.defaultFormatter": "honobonosun.language-hsp3",
	"editor.comments.insertSpace": false,
	"editor.experimental.pasteActions.enabled": true,
	"editor.folding": false,
	"editor.foldingHighlight": false,
	"editor.glyphMargin": false,
	"editor.guides.bracketPairs": true,
	"editor.guides.bracketPairsHorizontal": true,
	"editor.guides.highlightActiveIndentation": "always",
	"editor.inlayHints.fontFamily": "MyricaM M",
	"editor.inlayHints.padding": true,
	"editor.letterSpacing": 1,
	"editor.lineHeight": 24,
	"editor.maxTokenizationLineLength": 6000000,
	"editor.mouseWheelZoom": true,
	"editor.parameterHints.cycle": true,
	"editor.peekWidgetDefaultFocus": "editor",
	"editor.renderLineHighlight": "all",
	"editor.scrollbar.horizontalScrollbarSize": 5,
	"editor.scrollbar.verticalScrollbarSize": 10,
	"editor.rulers": [
		20,
		40,
		60,
		80
	],
	"editor.smartSelect.selectLeadingAndTrailingWhitespace": false,
	"editor.stickyScroll.enabled": true,
	"editor.wordWrapColumn": 100,
	"editor.cursorSmoothCaretAnimation": true,
	"editor.cursorWidth": 5,
	"editor.formatOnPaste": true,
	"editor.formatOnSave": true,
	"editor.formatOnType": true,
	"diffEditor.codeLens": true,
	"editor.minimap.autohide": true,
	"editor.minimap.enabled": false,
	"editor.minimap.maxColumn": 100,
	"editor.minimap.renderCharacters": false,
	"editor.minimap.size": "fit",
	"editor.snippetSuggestions": "top",
	"editor.suggest.preview": true,
	"editor.suggest.shareSuggestSelections": true,
	"editor.suggest.showStatusBar": true,
	"editor.suggest.showWords": false,
	"files.autoGuessEncoding": true,
	"files.trimTrailingWhitespace": true,
	"explorer.fileNesting.enabled": true,
	"search.useParentIgnoreFiles": true,
	"search.useGlobalIgnoreFiles": true,
	"scm.alwaysShowActions": true,
	"scm.alwaysShowRepositories": true,
	"scm.defaultViewSortKey": "name",
	"scm.diffDecorationsGutterPattern": {
		"added": true
	},
	"scm.inputFontFamily": "MyricaM M",
	"scm.providerCountBadge": "visible",
	"scm.repositories.sortOrder": "name",
	"css.format.spaceAroundSelectorSeparator": true,
	"git.alwaysShowStagedChangesResourceGroup": true,
	"git.alwaysSignOff": true,
	"git.closeDiffOnOperation": true,
	"git.enableCommitSigning": false,
	"git.pullTags": false,
	"git.repositoryScanMaxDepth": -1,
	"git.supportCancellation": true,
	"git.suggestSmartCommit": false,
	"git.useCommitInputAsStashMessage": true,
	"git-graph.fileEncoding": "Shift_JIS",
	"gitHistory.includeRemoteBranches": true,
	"language-hsp3.choiceWorkDirCur": true,
	"language-hsp3.compiler": "C:/HSP.git/hsp37beta/hspc.exe",
	"language-hsp3.encoding": "Shift_JIS",
	"language-hsp3.executor.enable": true,
	"language-hsp3.executor.index": "3.7",
	"language-hsp3.executor.paths": {
		"3.7": {
			"hide": false,
			"path": "C:/HSP.git/hsp37beta/hspc.exe/",
			"encoding": "Shift_JIS",
			"buffer": 204800,
			"helpman": "C:/HSP.git/hsp37beta/helphsp/helpman.exe/",
			"commands": {
				"run": [
					"-dwCra",
					"%FILEPATH%"
				],
				"make": [
					"-PmCa",
					"%FILEPATH%"
				]
			}
		}
	},
	"language-hsp3.helpman.path.local": "C:/HSP.git/hsp37beta/hsphelp/helpman.exe",
	"language-hsp3.helpman.path.online": "https://docs.hsp.moe/3.6/?q=%s",
	"html.format.templating": true,
	"markdown.experimental.validate.enabled": true,
	"markdown.preview.fontFamily": "MyricaM M",
	"javascript.format.insertSpaceAfterOpeningAndBeforeClosingTemplateStringBraces": true,
	"javascript.format.insertSpaceAfterOpeningAndBeforeClosingNonemptyParenthesis": true,
	"javascript.format.insertSpaceAfterOpeningAndBeforeClosingNonemptyBrackets": true,
	"javascript.format.insertSpaceAfterOpeningAndBeforeClosingJsxExpressionBraces": true,
	"javascript.format.insertSpaceAfterConstructor": true,
	"javascript.format.insertSpaceBeforeFunctionParenthesis": true,
	"typescript.format.insertSpaceAfterConstructor": true,
	"typescript.format.insertSpaceAfterOpeningAndBeforeClosingNonemptyBrackets": true,
	"typescript.format.insertSpaceAfterOpeningAndBeforeClosingTemplateStringBraces": true,
	"typescript.format.insertSpaceAfterOpeningAndBeforeClosingNonemptyParenthesis": true,
	"typescript.format.insertSpaceAfterOpeningAndBeforeClosingJsxExpressionBraces": true,
	"typescript.format.insertSpaceAfterTypeAssertion": true,
	"typescript.format.insertSpaceBeforeFunctionParenthesis": true,
	"editor.autoIndent": "full",
	"editor.tabSize": 4,
	"[hsp3]": {
		"editor.defaultFormatter": "vain0x.hsp3-analyzer-mini"
	},
	"language-hsp3.helpman.enable": "online",
	"explorer.excludeGitIgnore": false,
	"[markdown]": {
		"editor.defaultFormatter": "yzhang.markdown-all-in-one"
	},
	"files.encoding": "shiftjis"
}